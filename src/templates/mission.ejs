<% if (m.bonusItems.length) { -%>
{{itembox|You will receive:
<% m.bonusItems.forEach(function(item, index) { -%>
<% if (item.quantity) { %>|q<%- (index + 1) %>=<%- item.quantity %><% } %>|<%- item.name %>
<% }) -%>
}}
<% } -%>

<% if (m.hasNonItemRewards()) { -%>
You will <% if (m.bonusItems.length) { -%>also <% } -%>receive:
<% if (m.bonusXP) { -%>
* <%- m.getBonusXPStr() %> bonus follower XP
<% } -%>
<% if (m.money) { -%>
* {{cost|<%- m.getGold() %>|<%- m.getSilver() %>|<%- m.getCopper() %>}}
<% } -%>
<% if (m.bonusResources) { -%>
* [[<%- m.getResourceName() %>]] (<%- m.bonusResources %>)

<% } -%>
<% } -%>
<% } -%>
