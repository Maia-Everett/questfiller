{
	"manifest_version": 2,
	"name": "QuestFiller",
	"version": "2.0",

	"description" : "This is not a description.",

	"icons" : {
		"48" : "icons/questfiller-48.png",
		"96" : "icons/questfiller-48.png",
		"128" : "icons/questfiller-128.png"
	},

	"permissions" : [
		"tabs",
		"activeTab"
	],

	"background" : {
		"scripts" : [
			"background.js"
		]
	},

	"page_action": {
		"browser_style": true,
		"default_icon": {
			"48" : "icons/questfiller-48.png",
			"96" : "icons/questfiller-48.png",
			"128" : "icons/questfiller-128.png"
			},
		"default_title": "QuestFiller: Parse current page"
	  }
}
